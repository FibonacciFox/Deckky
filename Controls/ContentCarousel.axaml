<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Deckky.Controls.ContentCarousel"
             Background="Transparent">
    <Carousel HorizontalAlignment="Center" VerticalAlignment="Center" Name="Carousel">
        <Carousel.PageTransition>
            <PageSlide Duration="0.25" Orientation="Horizontal" />
        </Carousel.PageTransition>
        <TextBlock>ВСЕ ИГРЫ</TextBlock>
        <TextBlock>WINDOWS</TextBlock>
        <TextBlock>WINE|LINUX</TextBlock>
        <TextBlock>ЗАГРУЗКА</TextBlock>
        <TextBlock>ПРОФИЛЬ</TextBlock>
    </Carousel>
</UserControl>