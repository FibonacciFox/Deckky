<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:deckky="clr-namespace:Deckky.Controls"
        mc:Ignorable="d" 
        d:DesignWidth="1280" 
        d:DesignHeight="800"
        Width="1280" Height="800" MinWidth="1280" MinHeight="800"
        Title="Deckky"
        x:Class="Deckky.MainWindow">
    <StackPanel Background="#1A1E25" >
        <deckky:NavigationBar Name="NavigationBar" ActiveItem="0"/>
        <Carousel HorizontalAlignment="Center" VerticalAlignment="Center" Name="CarouselPage">
            <Carousel.PageTransition>
                <PageSlide Duration="0.25" Orientation="Horizontal" />
            </Carousel.PageTransition>
            <deckky:GamesPage/>
            <deckky:GamesPage/>
            <deckky:GamesPage/>
            <deckky:GamesPage/>
            <deckky:GamesPage/>
        </Carousel>
    </StackPanel>
</Window>